	public static void main(String[] args) {
		
		int intAnoF;
		String StrModelo;
		String StrMontadora;
		
		for (int veiculo = 0; veiculo < 5; veiculo++) {
			Veiculo v = new Veiculo();
			intAnoF = Integer.parseInt(JOptionPane.showInputDialog("Qual o ano de fabricação do veículo ?"))
			StrModelo = JOptionPane.showInputDialog("Qual a marca do veículo ?");
			StrMontadora = JOptionPane.showInputDialog("Qual a montadora do veículo ?");
			
		}
		
	}
}
